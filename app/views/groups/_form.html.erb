    <%= form_for @group do |f| %>
      <%= f.text_field :name, class: "form-control", placeholder: "グループ名を入力してください" %>
      <%= f.label :image,"画像" %>
      <%= f.file_field :image, class: "form-control", placeholder: "写真" %>
      <%= f.text_field :content, class: "form-control", placeholder: "説明" %>
      <%= f.submit "Save", class: "btn btn-success" %>
    <% end %>